{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
    }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],{
  "redirects": [
    {
      "source": "/:path*",
      "destination": "/",
      "statusCode": 200
    }
  ]
}

}
